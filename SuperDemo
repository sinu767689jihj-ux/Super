class Animal{
    String name;
    Animal(String name)
    {
        this.name=name;
    }
    void display()
    {
        System.out.println("Animal Name:"+name);
    }
}
class Dog extends Animal{
    String breed;
    Dog(String name,String breed)
    {
        super(name);
        this.breed=breed;
    }
    void displayDog()
    {
        display();
        System.out.println("Breed:"+breed);
    }
}
class Super{
    public static void main(String[] args)
    {
        Dog d=new Dog("pinky","uiw");
        d.displayDog();
    }
}
